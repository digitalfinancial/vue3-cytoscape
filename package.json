{
  "name": "vue3-cytoscape",
  "type": "module",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "npm run build",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "test:unit": "vite test:unit"
  },
  "dependencies": {
    "@types/cytoscape": "^3.8.0",
    "@types/uuid": "^3.4.6",
    "core-js": "^3.6.5",
    "cytoscape": "^3.28.1",
    "uuid": "^3.3.3",
    "vue": "^3.4.15"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/vue3-cytoscape.umd.cjs",
  "module": "./dist/vue3-cytoscape.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue3-cytoscape.js",
      "require": "./dist/vue3-cytoescap.umd.cjs"
    }
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.6.2",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^8.7.1",
    "path": "^0.12.7",
    "rimraf": "^5.0.5",
    "rollup-plugin-typescript2": "^0.36.0",
    "sass": "^1.70.0",
    "vite": "^4.5.2",
    "vite-plugin-css-injected-by-js": "^3.4.0",
    "vite-plugin-dts": "^3.7.3",
    "vitepress": "1.0.0-beta.7"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  }
}
